{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,

  "outputDirectory": "client",

  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://uw-love-flow.onrender.com/$1"
    },
    {
      "src": "^/([^\\.]+)\\.w$",
      "dest": "/api/templates/$1.html"
    },
    {
      "src": "/(.*)",
      "dest": "client/$1"
    }
  ]
}